        <attr name="bigColor" format="color"/> <!--外圈颜色-->
        <attr name="centerTextSize" format="dimension"/><!--中间字体颜色-->
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="matProg_progressIndeterminate" format="boolean" />
